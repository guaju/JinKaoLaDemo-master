<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:clickable="true"
    >

    <LinearLayout
        android:id="@+id/wode_layout"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        >
        <ImageView
            android:id="@+id/wodetouxiang"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/touxiangxiao"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="22dp"
            />
        <TextView
            android:id="@+id/nicheng"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="陈东"
            android:textColor="@color/text_color_black"
            android:textSize="12sp"
            android:layout_marginLeft="19dp"
            android:layout_marginTop="36dp"
            />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/wodeweike_layout"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        >
        <ImageView
            android:id="@+id/wodeweikeimg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/wodeweike"
            android:layout_marginLeft="51dp"
            android:layout_marginTop="34dp"
            />
        <TextView
            android:id="@+id/wodeweike"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的微课"
            android:textSize="12sp"
            android:textColor="@color/text_color_black"
            android:layout_marginTop="37dp"
            android:layout_marginLeft="21dp"
            />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/shangchuanliebiao_layout"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        >
        <ImageView
            android:id="@+id/shangchuanliebiao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="51dp"
            android:layout_marginTop="21dp"
            android:src="@drawable/shangchaunliebiao"
            />
        <TextView
            android:id="@+id/shangchuan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="上传列表"
            android:textColor="@color/text_color_black"
            android:textSize="12sp"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="21dp"
            />

    </LinearLayout>



    <LinearLayout
        android:id="@+id/shezhi_layout"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        >
        <ImageView
            android:id="@+id/shezhiimg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="51dp"
            android:layout_marginTop="21dp"
            android:src="@drawable/shezhi"
            />
        <TextView
            android:id="@+id/shezhi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设置"
            android:textSize="12sp"
            android:textColor="@color/text_color_black"
            android:layout_marginTop="25sp"
            android:layout_marginLeft="21dp"
            />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/bangzhu_layout"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        >
        <ImageView
            android:id="@+id/bangzhuimg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="51dp"
            android:layout_marginTop="21dp"
            android:src="@drawable/shangchaunliebiao"
            />
        <TextView
            android:id="@+id/bangzhu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="帮助"
            android:textSize="12sp"
            android:textColor="@color/text_color_black"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="21dp"
            />

    </LinearLayout>
    <android.support.v4.widget.Space
        android:layout_height="0dp"
        android:layout_width="match_parent"
        android:layout_weight="1"
        />

    <TextView
        android:id="@+id/tuichudenglu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="21dp"
        android:textColor="@color/text_color_red"
        android:text="退出"
        android:textSize="14sp"
        />

</LinearLayout>
