<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="210dp"
        android:layout_height="32dp"
        android:orientation="horizontal"
        android:background="@drawable/gongjulanbeijing"

        >
        <ImageView
            android:id="@+id/shexiangtou_img"
            android:layout_width="42dp"
            android:layout_height="match_parent"
            android:padding="4dp"
            android:layout_gravity="center"
            android:src="@drawable/shexiangtou"
            />
        <ImageView
            android:id="@+id/paizhao_img"
            android:layout_width="42dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:padding="4dp"
            android:src="@drawable/paizhao"
            />
        <ImageView
            android:id="@+id/jietu_img"
            android:layout_width="42dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:padding="4dp"
            android:src="@drawable/jietu"
            />
        <ImageView
            android:id="@+id/ziti_img"
            android:layout_width="42dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:padding="5dp"
            android:src="@drawable/ziti"
            />
        <ImageView
            android:id="@+id/beijing_img"
            android:layout_width="42dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:padding="5dp"
            android:src="@drawable/beijing"
            />
    </LinearLayout>
</LinearLayout>